    #if(${property.type} == "String"||${property.type} == "Date")
    if (self.str${codeName}) {
        [mDic setObject:self.str${codeName} forKey:@"${property.name}"];
    }
    #elseif(${property.type} == "double" || ${property.type} == "Double" || ${property.type} == "short" || ${property.type} == "Short" || ${property.type} == "int" || ${property.type} == "Integer"||${property.type} == "long"||${property.type} == "Long"||${property.type} == "float"||${property.type} == "Float")
